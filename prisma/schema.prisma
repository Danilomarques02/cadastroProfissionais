// conex√µes
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// Tabelas Banco de dados 

model cadastro {
  id   String @id @default(uuid())
  nomecompleto String
  cpf String
  profissao String
  celular Int 
  endereco String
  experiencia String
  aniversario String
  genero String
}
